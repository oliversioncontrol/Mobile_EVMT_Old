                            <Label Text="Boothcard" TextColor="#71D64A" FontAttributes="Bold"  VerticalOptions="Center" HorizontalOptions="Start" Font="Calibri" Grid.Row="16" Grid.Column="0" Margin="5,10,0,10">
                                <Label.FontSize>
                                    <OnIdiom x:TypeArguments="x:Double" Phone="12" Tablet="18"/>
                                </Label.FontSize>
                            </Label>
                            <Label Text="{Binding Boothcard}" VerticalOptions="Center" HorizontalOptions="EndAndExpand" Font="Calibri" Grid.Row="16" Grid.Column="1" LineBreakMode="TailTruncation" Margin="0,10,5,10">
                                <Label.FontSize>
                                    <OnIdiom x:TypeArguments="x:Double" Phone="12" Tablet="18"/>
                                </Label.FontSize>
                            </Label>
