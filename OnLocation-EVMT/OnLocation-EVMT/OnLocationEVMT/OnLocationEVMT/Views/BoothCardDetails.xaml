                                <Label.FontSize>
                                    <OnIdiom x:TypeArguments="x:Double" Phone="12" Tablet="18"/>
                                </Label.FontSize>
                            </Label>
                            <Label Text="{Binding ReviewedBy}" VerticalOptions="Center" HorizontalOptions="EndAndExpand" Font="Calibri" Grid.Row="18" Grid.Column="1" LineBreakMode="TailTruncation" Margin="0,10,5,10">
                                <Label.FontSize>
                                    <OnIdiom x:TypeArguments="x:Double" Phone="12" Tablet="18"/>
                                </Label.FontSize>
